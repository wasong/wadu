{
  "name": "wadu",
  "version": "0.0.1",
  "scripts": {
    "start": "node tools/run.js",
    "build": "NODE_ENV=production node tools/build.js --release",
    "test": "yarn run test:lint",
    "test:lint": "eslint src test tools postcss.config.js webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "Andrew Song <andrewgnos@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": ""
  },
  "sideEffects": [
    "*.css"
  ],
  "dependencies": {
    "@babel/polyfill": "7.11.5",
    "@hot-loader/react-dom": "16.13.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "apollo-cache-inmemory": "1.6.6",
    "apollo-client": "2.6.10",
    "apollo-link": "1.2.14",
    "apollo-link-context": "1.0.20",
    "apollo-link-error": "1.1.13",
    "apollo-link-http": "1.5.17",
    "apollo-link-retry": "2.2.16",
    "apollo-utilities": "1.3.4",
    "auth0-js": "9.13.4",
    "date-fns": "2.16.1",
    "graphql": "15.3.0",
    "graphql-tag": "2.11.0",
    "immutability-helper": "3.1.1",
    "jump.js": "1.0.2",
    "jwt-decode": "2.2.0",
    "lodash.debounce": "^4.0.8",
    "node-fetch": "3.1.1",
    "prop-types": "15.7.2",
    "querystring": "^0.2.0",
    "react": "^16.13.1",
    "react-apollo": "3.1.5",
    "react-dom": "^16.13.1",
    "react-helmet": "6.1.0",
    "react-hot-loader": "^4.12.21",
    "react-loadable": "5.5.0",
    "react-redux": "7.2.1",
    "react-router-dom": "5.2.0",
    "redux": "4.0.5",
    "redux-logger": "3.0.6",
    "redux-thunk": "2.3.0",
    "short-uuid": "^3.1.1",
    "url-join": "4.0.1",
    "whatwg-fetch": "3.4.0"
  },
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-do-expressions": "7.10.4",
    "@babel/plugin-proposal-export-default-from": "7.10.4",
    "@babel/plugin-proposal-export-namespace-from": "7.10.4",
    "@babel/plugin-proposal-function-bind": "7.11.5",
    "@babel/plugin-proposal-function-sent": "7.10.4",
    "@babel/plugin-proposal-json-strings": "7.10.4",
    "@babel/plugin-proposal-logical-assignment-operators": "7.11.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.10.4",
    "@babel/plugin-proposal-numeric-separator": "7.10.4",
    "@babel/plugin-proposal-optional-chaining": "7.11.0",
    "@babel/plugin-proposal-pipeline-operator": "7.10.5",
    "@babel/plugin-proposal-throw-expressions": "7.10.4",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-syntax-import-meta": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@babel/register": "7.11.5",
    "@babel/runtime": "7.11.2",
    "assets-webpack-plugin": "5.1.1",
    "autoprefixer": "9.8.6",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.3.0",
    "babel-loader": "8.1.0",
    "browser-sync": "2.26.12",
    "connect-history-api-fallback": "1.6.0",
    "css-loader": "4.2.2",
    "cssnano": "4.1.10",
    "ejs": "3.1.5",
    "enzyme": "3.11.0",
    "eslint": "7.8.1",
    "eslint-config-airbnb": "18.2.0",
    "eslint-import-resolver-webpack": "0.12.2",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jsx-a11y": "6.3.1",
    "eslint-plugin-react": "7.20.6",
    "file-loader": "6.1.0",
    "front-matter": "4.0.2",
    "jest": "26.4.2",
    "json-loader": "0.5.7",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "path-to-regexp": "6.1.0",
    "pixrem": "5.0.0",
    "pleeease-filters": "4.0.0",
    "postcss": "7.0.32",
    "postcss-calc": "7.0.4",
    "postcss-color-function": "4.1.0",
    "postcss-custom-media": "7.0.8",
    "postcss-custom-properties": "9.1.1",
    "postcss-custom-selectors": "5.1.2",
    "postcss-flexbugs-fixes": "4.2.1",
    "postcss-import": "12.0.1",
    "postcss-loader": "3.0.0",
    "postcss-media-minmax": "4.0.0",
    "postcss-nesting": "7.0.1",
    "postcss-selector-matches": "4.0.0",
    "postcss-selector-not": "4.0.0",
    "react-test-renderer": "16.13.1",
    "rimraf": "3.0.2",
    "style-loader": "1.2.1",
    "terser-webpack-plugin": "4.1.0",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "4.1.0",
    "webp-loader": "0.6.0",
    "webpack": "4.44.1",
    "webpack-dev-middleware": "3.7.2",
    "webpack-hot-middleware": "2.25.0",
    "webpack-merge": "5.1.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "react-hot-loader/babel",
      "@babel/plugin-transform-runtime",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions",
      "@babel/plugin-proposal-export-default-from",
      "@babel/plugin-proposal-logical-assignment-operators",
      "@babel/plugin-proposal-optional-chaining",
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "minimal"
        }
      ],
      "@babel/plugin-proposal-nullish-coalescing-operator",
      "@babel/plugin-proposal-do-expressions",
      "@babel/plugin-proposal-function-bind"
    ]
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "airbnb",
    "plugins": [
      "babel"
    ],
    "env": {
      "browser": true,
      "node": true
    },
    "globals": {
      "test": true,
      "expect": true,
      "jest": true
    },
    "rules": {
      "semi": [
        2,
        "never"
      ],
      "react/jsx-curly-newline": "off",
      "react/jsx-filename-extension": "off",
      "react/jsx-one-expression-per-line": "off",
      "max-len": "off",
      "babel/no-unused-expressions": 1,
      "react/prop-types": [
        1
      ],
      "global-require": "off",
      "no-console": "off",
      "import/no-extraneous-dependencies": [
        "error",
        {
          "devDependencies": true
        }
      ]
    },
    "settings": {
      "import/resolver": {
        "webpack": {
          "config": "./tools/webpack.common.js"
        }
      }
    }
  }
}
